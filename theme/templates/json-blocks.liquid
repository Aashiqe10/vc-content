{% assign blocks = page.content | parse_json %}

{% assign settings = blocks | first %}

{% for item in blocks -%}
    {% if item.hidden != 'True' %}
        {% assign templateName = "blocks/json-"|append:item.type -%}
        <!-- {{ item.type }} -->
        {% include templateName block: item, settings: settings -%}
    {% endif %}
{% endfor -%}
