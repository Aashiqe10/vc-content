    <link rel="canonical" href="{{ canonical_url }}" />
    <base href="{{ '/' | absolute_url }}" />
    <script>
        (function() {
            var _fbq = window._fbq || (window._fbq = []);
            if (!_fbq.loaded) {
                var fbds = document.createElement('script');
                fbds.async = true;
                fbds.src = '//connect.facebook.net/en_US/fbds.js';
                var s = document.getElementsByTagName('script')[0];
                s.parentNode.insertBefore(fbds, s);
                _fbq.loaded = true;
            }
            _fbq.push(['addPixelId', '{{ application_settings.FacebookTrackerId }}']);
        })();
        window._fbq = window._fbq || [];
        window._fbq.push(['track', 'PixelInitialized', {}]);
    </script>
    {% if settings.freshsales_enable %}
        {% include 'freshsales' %}
    {% endif %}
    <script>
        hljs.initHighlightingOnLoad();
    </script>
<noscript>
    <img height="1" width="1" alt="" style="display:none" src="{{ '//www.facebook.com/tr?id=' | append: application_settings.FacebookTrackerId | append: '&amp;ev=PixelInitialized' }}" />
</noscript>
</head>
<body ng-controller="mainController">
    <div id="layout">
        <div class="nav">
            <div class="vc-logo">
                <a href="docs">
                    <img src="{{ 'virtocommerce.png' | asset_url }}" alt="">
                    Virtocommerce Documentation
                </a>
            </div>
            <form action="" class="search" method="post">
                <input name="q" placeholder="Search docs" type="search" />
            </form>
            {% assign page_url_parts = page.handle | split: '/' %}
            {% for page_url_part in page_url_parts %}
                {% if page_url_part == 'vc2devguide' %}
                    {% include 'docs-dev-guide-pages' %}
                    {% break %}
                {% endif %}
                {% if page_url_part == 'vc2userguide' %}
                    {% include 'docs-user-guide-pages' %}
                    {% break %}
                {% endif %}
            {% endfor %}
        </div>
        <div class="cnt">
            {{ content_for_layout }}
        </div>
    </div>
    <script type="text/javascript">
        var ShareIt = {
            facebook: function (url) {
                window.open('https://www.facebook.com/sharer/sharer.php?u=' + encodeURIComponent(url), '_blank');
            },
            google: function (url) {
                window.open('https://plus.google.com/share?url=' + encodeURIComponent(url), '_blank');
            },
            twitter: function (url) {
                window.open('https://twitter.com/home?status=' + encodeURIComponent(url), '_blank');
            },
            linkedIn: function (url) {
                window.open('https://www.linkedin.com/cws/share?url=' + encodeURIComponent(url), '_blank');
            }
        }

        $('.menu .menu-item').each(function() {
            var self = $(this).find('a').attr('href');

            if(self == location.pathname.substr(1)) {
                $(this).addClass('active');
                $('.scroll-pane').animate({scrollTop: $(this).offset().top})
            }
        });

        $('.scroll-pane').jScrollPane({
            autoReinitialise: true
        });
    </script>
    <script src="//apis.google.com/js/platform.js" async defer></script>
    <script>
        $(function () {
            var items = $('.menu.__business-features li a');
            for (var i = 0; i < items.length; i++) {
                $(items[i]).parent().removeClass('__selected');
                if ($(items[i]).attr('href') === window.location.pathname.replace('/', '')) {
                    $(items[i]).parent().addClass('__selected');
                }
            }
        });
    </script>
</body>
</html>